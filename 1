const { validationResult } = require("express-validator");

module.exports.GetSocialUsers = async(req,res) => {
   const errors = validationResult(req);	
   const {} = req.query;
   if(errors.isEmpty()){	
       try{
	   res.status(200).json({ success: false, error: true, message: 'cccccccccccccccccccccc' });
       }catch(err){
           res.status(400).json({ success: false, error: true, errors: err.message });
       }
   }else{
       res.status(422).json({ success: false, error: true, message: errors.array() });
   }
};
